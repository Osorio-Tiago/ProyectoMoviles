<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AsoDesUnidos"
        tools:targetApi="31">
        <activity
            android:name=".activity.CustomerPrincipalActivity"
            android:exported="false" />
        <activity
            android:name=".activity.CustomerInformationActivity"
            android:exported="false" />
        <activity
            android:name=".activity.CustomerActivity"
            android:exported="false"
            android:label="@string/title_activity_customer"
            android:theme="@style/Theme.AsoDesUnidos.NoActionBar" />
        <activity
            android:name=".activity.AdminActivity"
            android:exported="false" />
        <activity
            android:name=".activity.Login"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AsoDesUnidos.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>