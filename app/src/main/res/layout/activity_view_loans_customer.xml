<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.ViewLoansCustomerActivity">

    <TextView
        android:id="@+id/plazoAdquirido"
        android:layout_width="105dp"
        android:layout_height="27dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="84dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vistaPrestamos" />

    <TextView
        android:id="@+id/interesAdq"
        android:layout_width="105dp"
        android:layout_height="27dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="84dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vistaPrestamos" />

    <TextView
        android:id="@+id/vistaPrestamos"
        android:layout_width="match_parent"
        android:layout_height="62dp"
        android:background="#015C56"
        android:clickable="false"
        android:text="@string/tittle_Prestamos"
        android:textAlignment="viewStart"
        android:textColor="#F1C9A0"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/montoPrestado"
        android:layout_width="105dp"
        android:layout_height="27dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vistaPrestamos" />

</androidx.constraintlayout.widget.ConstraintLayout>